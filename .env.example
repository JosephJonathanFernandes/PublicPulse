# Firebase Configuration Template
# Copy this file and rename to firebase_options.dart
# Fill in your actual Firebase configuration values

# ⚠️ NEVER commit the actual firebase_options.dart file to version control

# Instructions:
# 1. Go to Firebase Console: https://console.firebase.google.com/
# 2. Select your project
# 3. Go to Project Settings > General
# 4. Scroll down to "Your apps" section
# 5. Add an app for each platform (Web, Android, iOS)
# 6. Copy the configuration values below

# For Web:
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com

# For Android:
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here
FIREBASE_ANDROID_PACKAGE_NAME=com.example.publicpulse_2

# For iOS:
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_BUNDLE_ID=com.example.publicpulse2

# For macOS:
FIREBASE_MACOS_API_KEY=your_macos_api_key_here
FIREBASE_MACOS_APP_ID=your_macos_app_id_here
FIREBASE_MACOS_BUNDLE_ID=com.example.publicpulse2

# For Windows:
FIREBASE_WINDOWS_API_KEY=your_windows_api_key_here
FIREBASE_WINDOWS_APP_ID=your_windows_app_id_here

# How to use FlutterFire CLI to generate firebase_options.dart:
# 1. Install FlutterFire CLI:
#    dart pub global activate flutterfire_cli
#
# 2. Run configuration command:
#    flutterfire configure
#
# 3. Select your Firebase project
# 4. Select platforms you want to support
# 5. The CLI will generate firebase_options.dart automatically

# Additional Files Needed:
# - android/app/google-services.json (download from Firebase Console)
# - ios/Runner/GoogleService-Info.plist (download from Firebase Console)
# - macos/Runner/GoogleService-Info.plist (download from Firebase Console)
